				<script type="text/javascript">
						function warnTab(obj){
				        var timer = null;
				        obj.stop().animate({right:0},function(){
				            timer = setTimeout(function(){
				                obj.stop().animate({right: '-100%'},function(){
				                    clearTimeout(timer);
				                    timer = null;
				                });
				            },1000);
				        });
				    }
				    function checkMobile(mobile){
						var reg = /^1[3-9]{1}\d{9}$/;
						if(reg.test(mobile)){
							return true;
						}else{
							return false;
						}
					}
					//列表页、详情页右侧选型专家找软件
					$(".userMobile").focus(function(){
						$(this).css("color","#333");
						$(this).isNull = 1;
						if($(this).val() == "您的电话"){
							$(this).val("");
						}

					}).blur(function(){
						$(this).css("color","#a9a9a9");
						$(this).isNull = 0;
						if($(this).val() == ""){
							$(this).val("您的电话");
						}
					})
					$(".userName").focus(function(){
						$(this).css("color","#333");
						$(this).isNull = 1;
						if($(this).val() == "您的姓名"){
							$(this).val("");
						}

					}).blur(function(){
						$(this).css("color","#a9a9a9");
						$(this).isNull = 0;
						if($(this).val() == ""){
							$(this).val("您的姓名");
						}
					})
					//
					$('.btn-submit').click(function(){
						var mobile = $(".userMobile").val();
						var name = $(".userName").val();
						if(checkMobile(mobile)){
							$('.resultImg').show();
							$('.actForm').hide();
							//发送预约信息
							$.get("https://www.xuanruanjian.com/ajax_makeAnAppointment.php",{mobile:mobile,name:encodeURIComponent(name)},function(d){

							});
						}
						else{
							$(".userMobile").parent().find(".warn .txt").text("手机号错误");
							warnTab($(".userMobile").parent().find(".warn"));
						}
					});

				</script>
