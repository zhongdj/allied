	         <script type="text/javascript">

		         //5分钟找软件
				$(".header .search .code img,.scanscan").mouseover(function(){
					$(".scanscan").show();
				}).mouseout(function(){
					$(".scanscan").hide();
				});

				//搜索内容框js效果
				$('.search .btn-search').blur(function(){
					if($(this).val() == ''){
						$(this).val('');
					}
				});
				$('.search .btn-search').focus(function(){
					if($(this).val() == ''){
						$(this).val('');
					}
				});
				//按enter键提交
				$('.search .topQ').keyup(function(event){
					var keycode = event.which;
				    if (keycode == 13) {
				        submitSearchForm();
				    }
				});
				//提交搜索结果
				function submitSearchForm(){
					var qValue = $('[name=topQ]').val();
					var topSeOpt = $('[name=searchname]').val();
					// make url
					if( qValue == ''){
						return false;
					}
					if( topSeOpt == 1 ){
						var searchUrl = '/searchGoto.php?bc=utf-8&q=' + encodeURIComponent(qValue);
					}else{
						var searchUrl = '/serviceSearch.php?bc=utf-8&keyWords=' + encodeURIComponent(encodeURI(qValue));
					}
					document.location = searchUrl;
				}
				$('.kefugif').mouseover(function(){
					$(this).hide();
					$('.kefujpg').show();
					$('.kefupng').show();
				})
				$('.kefujpg').mouseout(function(){
					$(this).hide();
					$('.kefupng').hide();
					$('.kefugif').show();
				})
				$('.kefupng').mouseout(function(){
					$(this).hide();
					$('.kefujpg').hide();
					$('.kefugif').show();
				})
				$('.zixun').mouseover(function(){
					$('.zixunzi').show();
				})
				$('.zixun').mouseout(function(){
					$('.zixunzi').hide();
				})
				/* 根据用户登录判断是否展示购物车及订单
				var usrId = 0;
				if(usrId > 0){
					$(".header .head .quick dd a.loginShow").show();
				}else{
					$(".header .head .quick dd a.loginShow").hide();
				}
				*/

