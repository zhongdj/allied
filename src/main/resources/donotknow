			<script>
			function showBW8ChatWin(){
			$('.roundness_chat').click();
			resizeBW8ChatWin();
			}
			function resizeBW8ChatWin(){
			var bw8ChatWrap = $('#bw8-chatWrap'), t, l;
			t = ($(window).height()-bw8ChatWrap.height())/2+300;
			l = ($(window).width()-bw8ChatWrap.width())/2+500;
			if(t < 0) t = 0;
			if(l < 0) l = 0;
			bw8ChatWrap.css({top:t,left:l});
			}
			$(window).resize(function(){
			resizeBW8ChatWin();
			});
			</script>
